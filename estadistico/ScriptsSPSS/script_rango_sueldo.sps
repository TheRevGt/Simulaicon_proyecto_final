IF( salary<2500) RANGOSUELDO=1.
IF( salary>=2500 AND salary<5000) RANGOSUELDO=2.
IF( salary>=5000 AND salary<7500) RANGOSUELDO=3.
IF( salary>=7500 AND salary<10000) RANGOSUELDO=4.
IF( salary>10000) RANGOSUELDO=5.


FREQUENCIES VARIABLES=   RANGOSUELDO
  /BARCHART FREQ    
  /ORDER=ANALYSIS.

FREQUENCIES VARIABLES= RANGOSUELDO
  /PIECHART PERCENT        
  /ORDER=ANALYSIS    


